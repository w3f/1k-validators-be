environment: {{ .Environment.Name }}

domain: polkadot.w3f.community
dataPath: "/data"

backendPort: 3300

image:
  repo: web3f/otv-backend
 {{ if eq .Environment.Name "production" }}
  tag: {{ env "CIRCLE_TAG" }}
 {{ else }}
